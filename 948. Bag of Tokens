class Solution
{
public
    int bagOfTokensScore(int[] tokens, int power)
    {
        Arrays.sort(tokens);
        int low = 0, high = tokens.length - 1;
        int score = 0;
        while (low <= high)
        {
            if (tokens[low] <= power)
            {
                power = power - tokens[low];
                score += 1;
                low++;
            }
            else if (score > 0 && low != high)
            {
                power = power + tokens[high];
                score = score - 1;
                high--;
            }
            else
            {
                return score;
            }
        }
        return score;
    }
}
